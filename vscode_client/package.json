{
  "name": "go-swag-language-server",
  "displayName": "Go Swag Language Server Client",
  "description": "A VSCode extension for go-swag on LSP",
  "version": "0.0.2",
  "publisher": "takaaa220",
  "engines": {
    "vscode": "^1.95.0"
  },
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/takaaa220/go-swag-ide.git",
    "directory": "vscode_client"
  },
  "activationEvents": [
    "onLanguage:go"
  ],
  "contributes": {
    "languages": [
      {
        "id": "go",
        "aliases": [
          "Go",
          "go"
        ],
        "extensions": [
          ".go"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "check": "tsc -p . --noEmit",
    "publish": "vsce publish",
    "package": "vsce package"
  },
  "author": "takaaa220",
  "license": "Apache-2.0",
  "dependencies": {
    "@vscode/vsce": "^3.2.1",
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "typescript": "^5.7.2"
  }
}
